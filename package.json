    "@vitejs/plugin-react": "^4.0.4",
    "@yext/pages": "^1.0.0-rc.5",
    "autoprefixer": "^10.4.16",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "generate-license-file": "^2.0.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
