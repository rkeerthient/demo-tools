    "@yext/types": "0.1.12-alpha",
    "font-awesome": "^4.7.0",
    "mapbox-gl": "^2.15.0",
    "markdown-to-jsx": "^7.3.2",
    "react": "^18.2.0",
    "react-accessible-accordion": "^5.0.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-schemaorg": "^2.0.0",
    "react-showdown": "^2.3.1",
